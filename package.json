{
  "name": "jssm-viz-demo",
  "version": "1.1.1",
  "description": "Show how to use jssm-viz in a project, to render a jssm machine",
  "main": "dist/index.cjs.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -f jssm.es5.cjs.min.js && rm -f jssm-viz.es5.cjs.min.js",
    "clone": "cp ./node_modules/jssm/dist/jssm.es5.cjs.min.js . && cp ./node_modules/jssm-viz/dist/jssm-viz.es5.cjs.min.js .",
    "build": "npm run clean && npm run clone"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StoneCypher/jssm-viz-demo.git"
  },
  "keywords": [
    "jssm",
    "jssm-viz",
    "finite",
    "state",
    "machine",
    "finite_state_machine",
    "finite-state-machine",
    "fsm",
    "state-machine",
    "state_machine",
    "visualization",
    "demo",
    "viz"
  ],
  "author": "John Haugeland <stonecypher@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/StoneCypher/jssm-viz-demo/issues"
  },
  "homepage": "https://github.com/StoneCypher/jssm-viz-demo#readme",
  "dependencies": {},
  "devDependencies": {
    "jssm": "^4.4.2",
    "jssm-viz": "^1.0.0"
  }
}
