{
  "name": "jssm-viz-demo",
  "version": "1.3.5",
  "description": "Show how to use jssm-viz in a project, to render a jssm machine",
  "main": "dist/index.cjs.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf ./docs/ && mkdir docs",
    "cloneace": "cp -r ./node_modules/ace-builds/src/* ./docs",
    "clonejs": "cp ./node_modules/jssm/dist/jssm.es5.cjs.min.js ./docs/ && cp ./node_modules/jssm-viz/dist/jssm-viz.es5.cjs.js ./docs/",
    "clonehtml": "cp ./src/html/* ./docs/",
    "clone": "npm run clonejs && npm run clonehtml && npm run cloneace",
    "build": "npm run clean && npm run clone"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StoneCypher/jssm-viz-demo.git"
  },
  "keywords": [
    "jssm",
    "jssm-viz",
    "finite",
    "state",
    "machine",
    "finite_state_machine",
    "finite-state-machine",
    "fsm",
    "state-machine",
    "state_machine",
    "visualization",
    "demo",
    "viz"
  ],
  "author": "John Haugeland <stonecypher@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/StoneCypher/jssm-viz-demo/issues"
  },
  "homepage": "https://github.com/StoneCypher/jssm-viz-demo#readme",
  "dependencies": {
    "jssm": "^5.11.4",
    "jssm-viz": "^1.1.8"
  },
  "devDependencies": {
    "ace-builds": "^1.2.8",
    "eslint": "^4.8.0",
    "rimraf": "^2.6.2"
  }
}
